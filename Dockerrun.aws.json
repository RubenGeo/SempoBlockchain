{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:server",
      "essential": true,
      "memoryReservation": 256,
      "cpu": 1,
      "links": [
        ],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "APP"
        },
        {
          "name": "LOCATION",
          "value": "PROD"
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "proxy",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:proxy",
      "essential": true,
      "memory": 64,
      "cpu": 1,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "links": [
        "app:app"
      ]
    },
    {
      "name": "worker",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:worker",
      "essential": false,
      "memory": 256,
      "links": [
      ],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "WORKER"
        },
        {
          "name": "LOCATION",
          "value": "PROD"
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "beat",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:worker",
      "essential": false,
      "memory": 256,
      "links": [
      ],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "BEAT"
        },
        {
          "name": "LOCATION",
          "value": "PROD"
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    }
  ]
}